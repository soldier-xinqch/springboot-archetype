<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xinqch.springbootarchetype.mapper.TestTabMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xinqch.springbootarchetype.entity.TestTab">
        <id column="id" property="id" />
        <result column="message" property="message" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, message
    </sql>


    <select id="getById" parameterType="java.lang.String" resultType="com.xinqch.springbootarchetype.entity.TestTab" >
        select <include refid="Base_Column_List" />  from test_tab WHERE  id = #{id}
    </select>
</mapper>
